    <footer>
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <ul class="list-inline">
              <li>
                <a href="/">Home</a>
              </li>
              <li class="footer-menu-divider">&sdot;</li>
              <li>
                <a href="/about">About</a>
              </li>
              <li class="footer-menu-divider">&sdot;</li>
              <!--
              <li class="footer-menu-divider">&sdot;</li>
              <li>-->
                <a href="/contact">Contact</a>
              </li>
            </ul>
            <p class="copyright text-muted small">Copyright &copy; Your Company 2014. All Rights Reserved</p>
          </div>
        </div>
      </div>
    </footer>
</body>
</html>
